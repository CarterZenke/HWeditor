  "rubric": [
    {
      "title": "Computer Science",
      "type": "computer-science",
      "requirements": [
        {
          "id": "4956d501-8969-4a35-b7a5-f359afd50094",
          "text": "My project includes at least <b>two scripts</b>."
        },
        {
          "id": "15264eaf-4677-4811-8316-c5b2641f716d",
          "text": "My project includes a <samp data-coblocks-type='functions'>public function</samp> <b>with a parameter that is called in a different script</b>."
        },
        {
          "id": "238044a5-1d8a-4a12-b615-66e68d7d45e4",
          "text": "My project includes a <samp data-coblocks-type='data'>CoSpace property</samp> that is used to <b>store information across two scripts</b>."
        },
        {
          "id": "c8edd412-cffa-415a-b104-6845c278edaa",
          "text": "My project includes at least <b>two different types of <samp data-coblocks-type='control'>loops</samp></b>."
        },
        {
          "id": "636181af-2334-4c60-8cc9-ed5920c250b4",
          "text": "My project includes at least <b>two</b> <samp data-coblocks-type='control'>conditionals</samp>."
        },
        {
          "id": "6fbb36c4-17eb-4d5c-98ff-d1ccf7df3761",
          "text": "My project includes a <samp data-coblocks-type='control'>conditional</samp> that <b>uses the <samp data-coblocks-type='data'>length of a [list]</samp> in the condition</b>."
        },
        {
          "id": "26b78791-6754-4ca7-a5a1-639f0d5c7dc6",
          "text": "My project includes at least <b>three items that can be <samp data-coblocks-type='data'>added</samp> or <samp data-coblocks-type='data'>removed</samp> from a <samp data-coblocks-type='data'>list</samp> by clicking on the item</b>."
        }
      ]
    },
    {
      "title": "Interdisciplinary Learning",
      "type": "interdisciplinary-learning",
      "requirements": [
        {
          "id": "163baab8-fc31-433e-a198-646c622e60b5",
          "text": "My project includes <b>unique scenery, events, and characters</b>."
        }
      ]
    },
    {
      "title": "Creativity",
      "type": "creativity",
      "subtitle": "<b>One or more of the following:</b>",
      "requirements": [
        {
          "id": "415f2f92-844b-494c-95aa-aee61a6468de",
          "text": "<b>Demonstrates originality:</b> My project includes characters, animals, or other objects that are unusual, innovative, or clever."
        },
        {
          "id": "119f5729-fdf1-455a-92d2-af1dd2fabc2c",
          "text": "<b>Draws on other sources:</b> My project includes an engaging scenario based on my own creative vision or outside creative influences."
        },
        {
          "id": "c92acad0-fcbf-473c-9a64-df1415042bd9",
          "text": "<b>Excels in visual design:</b> My project includes characters, animals, or other objects that are thoughtfully placed in the scene."
        },
        {
          "id": "cedd4c7c-c296-4e9f-abfb-14db30d5f367",
          "text": "<b>Uses humor:</b> My project includes funny or impractical objects and characters."
        }
      ]
    }
  ],
